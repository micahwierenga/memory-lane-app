<div class='row col-md-3'>

	<form id='storyForm'>
		<div class='form-group'>
			<input class='form-control' type='text' placeholder='Street' name='street' id='street'>
			<input class='form-control' type='number' placeholder='Zip' name='zip' id='zip'>
		</div>
		<div class='form-group'>
			<label for="monthStart">Month</label>
			<input class='form-control' type='text' placeholder='From' name='monthStart' id='monthStart'>
			<input class='form-control' type='text' placeholder='To' name='monthEnd' id='monthEnd'>
		</div>

		<div class='form-group'>
			<label for="yearStart">Year</label>
			<input class='form-control' type='text' placeholder='From' name='yearStart' id='yearStart'>
			<input class='form-control' type='text' placeholder='To' name='yearEnd' id='yearEnd'>
		</div>

		<div class='form-group'>
			<input class='form-control' type='text' placeholder='Your Story' name='storyBody' id='storyBody'>
		</div>

		<div class='form-group'>
			<input class='btn btn-default' type='submit'>
		</div>
	</form>
</div>